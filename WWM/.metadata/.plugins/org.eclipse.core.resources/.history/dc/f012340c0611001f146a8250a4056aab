package de.bkbocholt.wwm;

public class Spiel 
{
	Frage[] fragenKatalog;

	public static void main(String[] args)
	{
		
	}

	public Spiel()
	{
		fragenKatalog = new Frage[5];
		String[] antworten = {"30","25","40","60"};
		fragenKatalog[0] = new Frage("Wie alt ist Justin?","30",antworten);
		fragenKatalog[1] = new Frage("Wie alt ist Marius?","30",antworten);
		fragenKatalog[2] = new Frage("Wie alt ist Peter?","30",antworten);
		fragenKatalog[3] = new Frage("Wie alt ist Justin?","30",antworten);
		fragenKatalog[4] = new Frage("Wie alt ist Justin?","30",antworten);
	}

}
